{
    "name": "theozebua/laravel-view-maker",
    "description": "A simple command to generate laravel views",
    "keywords": ["laravel", "artisan", "views", "templates", "blade"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "theozebua",
            "email": "ronizebua.rz@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "laravel/framework": "^8.0 || ^9.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9",
        "orchestra/testbench": "^7.7"
    },
    "autoload": {
        "psr-4": {
            "Theozebua\\LaravelViewMaker\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Theozebua\\LaravelViewMaker\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Theozebua\\LaravelViewMaker\\ServiceProvider"
            ]
        }
    }
}
